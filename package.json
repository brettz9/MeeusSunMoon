{
  "name": "meeussunmoon",
  "version": "2.1.2",
  "description": "Accurately calculate sunrise, sunset, and moon phases.",
  "main": "dist/meeussunmoon.min.js",
  "module": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "static -p 8049",
    "coverage": "open-cli http://localhost:8049/coverage/ && npm start",
    "eslint": "eslint .",
    "rollup": "rollup -c",
    "mocha": "_mocha --timeout 10000 test/tests.js --delay --require esm --require test/node-env.js",
    "browser-test": "open-cli http://localhost:8049/test/ && npm start",
    "node-test": "npm run eslint && npm run rollup && npm run mocha",
    "test": "npm run eslint && npm run rollup && rm -Rf node_modules/.cache && nyc npm run mocha"
  },
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "babel-eslint": "^11.0.0-beta.2",
    "chai": "^4.2.0",
    "eslint": "7.0.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "esm": "^3.2.25",
    "mocha": "^7.1.2",
    "node-static": "0.7.11",
    "nyc": "^15.0.1",
    "open-cli": "^6.0.1",
    "rollup": "2.10.2",
    "rollup-plugin-terser": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janrg/MeeusSunMoon.git"
  },
  "keywords": [
    "sunrise",
    "sunset",
    "solar",
    "noon",
    "moon",
    "phases",
    "astronomical",
    "calculations"
  ],
  "author": "Jan Greis",
  "contributors": [
    "Brett Zamir"
  ],
  "engines": {
    "node": ">=6.13.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janrg/MeeusSunMoon/issues"
  },
  "homepage": "https://github.com/janrg/MeeusSunMoon#readme"
}
